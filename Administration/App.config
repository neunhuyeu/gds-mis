<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAppointment" />
                <binding name="BasicHttpBinding_IDoctor" />
                <binding name="BasicHttpBinding_IAdministration" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Appointment_Serves/IAppointment/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAppointment"
                contract="Appointments.IAppointment" name="BasicHttpBinding_IAppointment" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/DMS_Service/IDoctor/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IDoctor"
                contract="MedicalInformation.IDoctor" name="BasicHttpBinding_IDoctor" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/DMS_Service/CAdministration/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdministration"
                contract="serverAdministration.IAdministration" name="BasicHttpBinding_IAdministration" />
        </client>
    </system.serviceModel>
</configuration>